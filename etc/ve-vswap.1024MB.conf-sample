VERSION="2"

ONBOOT="yes"

PHYSPAGES="262144:262144"
SWAPPAGES="262144"
# Virtual memory overcommitment
# privvmpages = ram + swap * VM_OVERCOMMIT (unlimited if not specified)
#VM_OVERCOMMIT="1.5"

NETFILTER="stateless"

CPUUNITS="1000"

DISKSPACE="10485760:10485760"
DISKINODES="655360:655360"
QUOTATIME="0"
IP_ADDRESS="0.0.0.0"

PLATFORM="linux"
